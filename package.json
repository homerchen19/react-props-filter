{
  "name": "react-props-filter",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "build":
      "npm run clean && cross-env NODE_ENV=production babel src -d lib --ignore **/__tests__",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "prepublish": "npm run build",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test"
  },
  "author": "xxhomey19",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "peerDependencies": {
    "react": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-ramda": "^1.6.1",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-yoctol": "^0.16.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.6.0",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2"
  },
  "keywords": ["react", "props", "filter"],
  "jest": {
    "collectCoverageFrom": ["src/**/*.js"],
    "testPathIgnorePatterns": ["/node_modules/", "<rootDir>/lib"],
    "resetModules": true,
    "resetMocks": true
  },
  "lint-staged": {
    "*.js$": ["eslint --fix", "git add"],
    "*.{json,md}": ["prettier --write", "git add"],
    "package.json": [
      "prettier-package-json --write",
      "prettier package.json --write",
      "git add"
    ],
    ".babelrc": ["prettier .babelrc --write", "git add"]
  }
}
